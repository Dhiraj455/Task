{{!-- <h3>{{viewTitle}}</h3> --}}
<div class="box1">
  <h3>Signup</h3>
  <br />
  <br />
  <form action="/signup" method="POST" autocomplete="off">
    <div class="row mb-3">
      <input type="hidden" name="id" id="" />
      <label for="inputEmail3" class="col-sm-2 col-form-label">Name</label>
      <div class="col-sm-10">
        <input type="text" name="name" class="form-control" />
      </div>
    </div>
    <div class="row mb-3">
      <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
      <div class="col-sm-10">
        <input
          type="email"
          class="form-control"
          name="email"
          id="inputEmail3"
        />
      </div>
    </div>
    <div class="row mb-3">
      <label
        for="inputPassword3"
        class="col-sm-2 col-form-label"
      >Password</label>
      <div class="col-sm-10">
        <input
          type="password"
          name="password"
          class="c form-control"
          id="inputPassword3"
        />
      </div>
    </div>
    {{! <a href="/userprofile">user</a> }}
    <button type="submit" class="btn btn-outline-danger">Sign up</button>
    <a href="/" class="btn btn-outline-danger">Home</a>
    <h5>Have Account ? <a href="/login" style="text-decoration: none; color:red">Sign In</a></h5>
  </form>
</div>